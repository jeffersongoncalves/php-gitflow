{
  "name": "jeffersonsimaogoncalves/php-gitflow",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jefferson Simão Gonçalves",
      "email": "gerson.simao.92@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/console": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "JeffersonSimaoGonalves\\PhpGitflow\\": "src/"
    }
  },
  "bin": [
    "bin/php-gitflow"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
